<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces
                      http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">

    <url-mapping id="user-login">
        <pattern value="/login"/>
        <view-id value="/jsf/login.xhtml"/>
    </url-mapping>

    <url-mapping id="user-register">
        <pattern value="/register"/>
        <view-id value="/jsf/register.xhtml"/>
    </url-mapping>

    <url-mapping id="packages-details">
        <pattern value="/packages/details"/>
        <view-id value="/jsf/details.xhtml"/>
    </url-mapping>

    <url-mapping id="receipts">
        <pattern value="/receipts"/>
        <view-id value="/jsf/receipts.xhtml"/>
    </url-mapping>

    <url-mapping id="ships">
        <pattern value="/ship"/>
        <view-id value="/jsf/receipt-details.xhtml"/>
    </url-mapping>

    <url-mapping id="package-create">
        <pattern value="/packages/create"/>
        <view-id value="/jsf/create-package.xhtml"/>
    </url-mapping>

    <url-mapping id="pending">
        <pattern value="/pending"/>
        <view-id value="/jsf/pending.xhtml"/>
    </url-mapping>

    <url-mapping id="delivered">
        <pattern value="/delivered"/>
        <view-id value="/jsf/delivered.xhtml"/>
    </url-mapping>

    <url-mapping id="shipped">
        <pattern value="/shipped"/>
        <view-id value="/jsf/shipped.xhtml"/>
    </url-mapping>

</pretty-config>